{
  "presets": [
    "es2015",
    "react",
    [
      "env",
      {
        "modules": false,
        "targets": {
          "IE": "11"
        },
        "useBuiltIns": true
      }
    ]
  ],
  "plugins": ["transform-class-properties"],
  "env": {
    "test": {
      "plugins": ["transform-es2015-modules-commonjs"]
    }
  }
}
